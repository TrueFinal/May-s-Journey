[dialogic]

directories/dch_directory={
"Carlos": "res://falas/dialogic/portraits/Carlos.dch",
"Josue": "res://falas/dialogic/Josue.dch",
"May": "res://falas/dialogic/May.dch"
}
directories/dtl_directory={
"Ato I ": "res://falas/dialogic/Ato I .dtl",
